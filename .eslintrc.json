{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": "standard-with-typescript",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "local-rules"
    ],
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "local-rules/no-invalid-language-identifiers": ["error", {"allowedLanguages": ["es"]}]
    }
  }